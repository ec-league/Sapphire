<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="actionBuilder" class="com.sapphire.biz.stock.algorithm.exec.ActionBuilder">
        <property name="shouldBuyList">
            <list>
                <value>basicAction</value>
                <value>macdAction</value>
            </list>
        </property>
        <property name="shouldSellList">
            <list>
                <value>basicAction</value>
                <value>sellJudgeAction</value>
            </list>
        </property>
    </bean>
</beans>